{
	"$schema": "http://json-schema.org/draft/2019-09/schema#",
	"id": "$NDISCHEMAPATH\/data\/ndi_document_binaryseries_schema.json",
	"title": "ndi_document_data_binaryseries",
	"type": "object",
	"properties": {
		"time_size": {
			"type": "number",
			"doc_default_value": "1",
			"doc_data_type": "Integer array [n m]",
			"doc_description": "Number of independent variable entries (often time)"
		},
		"time_type": {
			"type": "string",
			"doc_default_value": "",
			"doc_data_type": "character array (ASCII)",
			"doc_description": "Type of the time variable ('float64', 'uint32', etc) (note: make better)"
		},
		"data_size" : {
			"type" : "number",
			"doc_default_value": "1",
			"doc_data_type": "Integer array [n m]",
			"doc_description": "Number of data entries)"
		},
		"data_type" : {
			"type" : "string",
			"doc_default_value": "",
			"doc_data_type": "character array (ASCII)",
			"doc_description": "Type of the time variable ('float64', 'uint32', etc) (note: make better)"
		},
		"data_dim" : {
			"type" : "number",
			"doc_default_value": "1",
			"doc_data_type": "Integer array [n m]",
			"doc_description": "Dimensions of each data series"
		},
		"samples_regular_intervals" : {
			"type" : "number",
			"doc_default_value": "1",
			"doc_data_type": "Integer (0 or 1)",
			"doc_description": "is the data always sampled at regular intervals?"
		}
	}
}
